{
	"name": "react-wizardry",
	"displayName": "'React Wizardry'",
	"description": "Simplify your React development workflow with React Wizardry!",
	"version": "1.0.1",
	"type": "module",
	"publisher": "nhope",
	"repository": "https://github.com/nhope123/react-wizardry.git",
	"keywords": [
		"react",
		"vite",
		"typescript",
		"mui",
		"i18n"
	],
	"engines": {
		"vscode": "^1.95.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "react-wizardry.createComponent",
				"title": "Create Component",
				"category": "RWiz"
			},
			{
				"command": "react-wizardry.createHook",
				"title": "Create React Hook",
				"category": "RWiz",
				"icon": "./src/assets/wizard.svg"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint .",
		"test": "vitest"
	},
	"devDependencies": {
		"@types/mock-require": "^3.0.0",
		"@types/node": "22.10.1",
		"@types/vscode": "^1.95.0",
		"@typescript-eslint/eslint-plugin": "^8.16.0",
		"@typescript-eslint/parser": "^8.16.0",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.4.1",
		"eslint": "^9.16.0",
		"mock-require": "^3.0.3",
		"typescript": "^5.7.2",
		"vite-tsconfig-paths": "^5.1.3",
		"vitest": "^2.1.6"
	},
	"dependencies": {
		"@vscode/vsce": "^3.2.1",
		"global": "^4.4.0"
	}
}
